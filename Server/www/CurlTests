curl -i http://localhost:8080/answer -X PUT -H "Content-Type: application/json" -d '{"discourseId":"1234567", "reference":"John6", "teacherId":"XXXX", "message":"This is the answer"}' #409

curl -i http://localhost:8080/answer -X PUT -H "Content-Type: application/json" -d '{"discourseId":"12345", "reference":"John6", "teacherId":"ABCDE", "message":"This is the answer"}' #201

curl -i http://localhost:8080/answer -X POST -H "Content-Type: application/json" -d '{"messageId":4, "reference":"John7", "message":"This is the revised answer"}' #200

curl -i http://localhost:8080/answer -X POST -H "Content-Type: application/json" -d '{"messageId":1000, "reference":"John7", "message":"This is the revised answer"}' #404

curl -i http://localhost:8080/answer -X DELETE -H "Content-Type: application/json" -d '{"messageId":4}' #200

curl -i http://localhost:8080/answer -X DELETE -H "Content-Type: application/json" -d '{"messageId":400}' #404



curl -i http://localhost:8080/draft -X PUT -H "Content-Type: application/json" -d '{"discourseId":"12345", "reference":"John7", "message":"This is a draft message"}' #200

curl -i http://localhost:8080/draft -X PUT -H "Content-Type: application/json" -d '{"discourseId":"123", "reference":"John8", "message":"This is a draft message"}' #409

curl -i http://localhost:8080/draft -X POST -H "Content-Type: application/json" -d '{"messageId":6, "reference":"John8", "message":"This is a revised draft message"}' #200

curl -i http://localhost:8080/draft -X POST -H "Content-Type: application/json" -d '{"messageId":600, "reference":"John8", "message":"This is a revised draft message"}' #409

curl -i http://localhost:8080/draft -X DELETE -H "Content-Type: application/json" -d '{"messageId":6}' #200

curl -i http://localhost:8080/draft -X DELETE -H "Content-Type: application/json" -d '{"messageId":6}' #404

curl -i http://localhost:8080/draft/5 #200

