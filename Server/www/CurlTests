curl -i http://localhost:8080/user -X PUT -H "Content-Type: application/json" -d '{"teacherId":"ABCDEFGHIJ", "fullname":"Gary Griswold", "pseudonym":"Gary G", "signature":"XXXX", "versionId":"KJV"}' # 201

curl -i http://localhost:8080/user -X PUT -H "Content-Type: application/json" -d '{"teacherId":"ABCDEFGHIJ", "fullname":"Gary Griswold", "pseudonym":"Gary G", "signature":"XXXX", "versionId":"KJV"}' #409

curl -i http://localhost:8080/user -X POST -H "Content-Type: application/json" -d '{"teacherId":"ABCDE", "fullname":"Gary N Griswold", "pseudonym":"Gary N"}' # 200

curl -i http://localhost:8080/user -X POST -H "Content-Type: application/json" -d '{"teacherId":"AB", "fullname":"Gary N Griswold", "pseudonym":"Gary N"}' # 404

curl -i http://localhost:8080/user/ABCDEFGHIJ -X DELETE # 200

curl -i http://localhost:8080/user/ABCDEFG -X DELETE # 404


