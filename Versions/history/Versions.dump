INSERT INTO "Version" VALUES('ERV-VIE','vie','BLI','ERV-VIE','Thánh Kinh: Bản Phổ thông','BIBLE','ERV-VIE.db','T','F','2016-10-12','Thánh Kinh: Bản Phổ thông (BPT) © 2010 Bible League International','  <p class="m">
    <span class="bd">Thánh Kinh: Bản Phổ thông (BPT)</span></p>
  <p class="m">© 2010 Bible League International</p>
