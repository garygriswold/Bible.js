INSERT INTO "Version" VALUES('ERV-HRV','hrv','BLI','ERV-HRV','Novi zavjet, Suvremeni prijevod','BIBLE_NT','ERV-HRV.db','T','F','2016-10-17','Novi zavjet: Suvremeni prijevod (SHP) © 2002 Bible League International','  <p class="m">
